
# Environment Configuration
NODE_ENV=development

# Server Configuration
PORT=5000
HOST=0.0.0.0

# CORS Configuration (optional - for production)
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:5000

# Yahoo Finance API (optional - uses yahoo-finance2 package by default)
# YAHOO_FINANCE_API_KEY=your_api_key_here

# OAuth Configuration (if using authentication)
# OAUTH_SERVER_URL=https://your-oauth-server.com

# Session Configuration (if using sessions)
# SESSION_SECRET=your-random-secret-here

# Database Configuration (if needed in future)
# DATABASE_URL=postgresql://user:password@localhost:5432/gann_db

# Logging
LOG_LEVEL=info
